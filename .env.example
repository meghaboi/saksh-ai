# Example environment variables for saksh-ai
# Duplicate this file to `.env.local` for local development

NEXTAUTH_SECRET=changeme-super-secret
# Comma-separated list, e.g. "github,google,email"
AUTH_PROVIDERS=github

# Database connection string
DATABASE_URL=postgresql://user:password@localhost:5432/saksh_ai

# Comma-separated storage keys used by the app
STORAGE_KEYS=session,upload,temp

# Resend email provider API key
RESEND_API_KEY=your_resend_key_here
